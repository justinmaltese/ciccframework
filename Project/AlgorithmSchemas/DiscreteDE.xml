<?xml version="1.0"?>
<algorithm id = "DDE" name="Discrete Differential Evolution" driverClass="com.ciccFramework.algorithms.dde.DiscreteDifferentialEvolutionAlgorithm">
	<parameterSet>
		<parameter id="MAX_ITERATIONS" label="Maximum number of iterations" type="Integer" defaultValue="200"/>
		<parameter id="POP_SIZE" label="Population size" type="Integer" defaultValue="400"/>	
		<parameter id="CROSSOVER_PROBABILITY" label="Crossover probability" type="Double" defaultValue="0.7"/>
		<parameter id="MUTATION_PROBABILITY" label="Crossover probability" type="Double" defaultValue="0.35"/>
		<parameter id="DIFFERENTIAL_WEIGHT" label="Differential weight" type="Double" defaultValue="1.2"/>	
	</parameterSet>
</algorithm>
