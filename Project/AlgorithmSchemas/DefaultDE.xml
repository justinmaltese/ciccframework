<?xml version="1.0"?>
<algorithm id = "DE" name="Differential Evolution" driverClass="com.ciccFramework.algorithms.de.DifferentialEvolutionAlgorithm">
	<parameterSet>
		<parameter id="MAX_ITERATIONS" label="Maximum number of iterations" type="Integer" defaultValue="200"/>
		<parameter id="POP_SIZE" label="Population size" type="Integer" defaultValue="400"/>	
